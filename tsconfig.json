    "module": "commonjs",
    "outDir": "lib",
    "sourceMap": true,
    "target": "ES6",
    "noEmitOnError": false,
    "resolveJsonModule": true,
  },
  "compileOnSave": true,
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/**"
  ]
}